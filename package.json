{
  "name": "upass-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "export NODE_ENV=local && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=prod && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/database.source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/database.source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/database.source.ts migration:revert",
    "migration:show": "npm run typeorm -- --dataSource=src/database/database.source.ts migration:show",
    "rpls": "./node_modules/rpls/bin/cli.js"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "10.0.2",
    "@nestjs/websockets": "^10.3.10",
    "axios": "^1.7.2",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dayjs": "^1.11.11",
    "generate-password": "^1.7.1",
    "handlebars": "4.7.8",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "6.9.14",
    "passport": "0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "4.0.1",
    "pg": "8.12.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "5.0.7",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^10.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.2",
    "@nestjs/testing": "10.3.10",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.6.7",
    "@types/bcryptjs": "2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "hygen": "6.2.11",
    "jest": "29.7.0",
    "prettier": "3.3.2",
    "rpls": "^1.0.16",
    "source-map-support": "^0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.3",
    "typescript": "5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
